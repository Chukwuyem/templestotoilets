{
	"name": "name_holder",
	"manifest_version": 2,
	"version": "1.0.0",

	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},

	"content_scripts": [{
		"matches": [ "<all_urls>" ],
		"run_at": "document_end",
		"all_frames": true,
		"js": ["content.js"]
	}],

	"permissions": ["tabs"]

}